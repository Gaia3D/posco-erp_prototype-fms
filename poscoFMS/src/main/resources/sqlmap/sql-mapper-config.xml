<configuration>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true" />
    </settings>
    <typeAliases>
    
        <typeAlias alias="GirderErrorStatus" type="kr.posco.erp.poscofms.main.vo.GirderErrorStatusVO"/>
        <typeAlias alias="GirderMeasurement" type="kr.posco.erp.poscofms.main.vo.GirderMeasurementVO"/>
